{
  "kind": "collectionType",
  "collectionName": "applications",
  "info": {
    "singularName": "application",
    "pluralName": "applications",
    "displayName": "application",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "phoneNo": {
      "type": "biginteger",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "resume": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "files"
      ]
    },
    "experience": {
      "type": "component",
      "repeatable": true,
      "component": "add-education.education"
    },
    "eeduField": {
      "displayName": "eeducation",
      "type": "component",
      "repeatable": true,
      "component": "add-education.eeducation"
    },
    "jobs": {
      "type": "relation",
