{
  "kind": "collectionType",
  "collectionName": "applications",
  "info": {
    "singularName": "application",
    "pluralName": "applications",
    "displayName": "application"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "address": {
      "type": "string",
      "required": true
    },
    "phoneNo": {
      "type": "biginteger",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "resume": {
      "allowedTypes": [
        "files"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "secondStep": {
      "displayName": " education",
      "type": "component",
      "repeatable": false,
      "component": "second-step.education"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "applications"
    },
    "jobs": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::job.job",
      "mappedBy": "applications"
    }
  }
}
